%p#notice= notice

%p
  %b Title:
  = @page.title
%p
  %b Content:
  = markdown @page.content
%p
  %b Owner:
  = @page.owner_id
%p
  %b Private:
  = @page.private
%hr
= form_tag "/pages/#{@page.id}/set_collaborators", method: :put do
  - @test_array.each do |num|
    .check_box_div
      = check_box_tag "collaborators[]", num, false
  = submit_tag
%hr

= link_to 'Edit', edit_page_path(@page)
\|
= link_to 'Back', pages_path
